{% extends 'overall/layout' %}
{# Título de la página #}
{% block appTitle %}Duraciones en las Atracciones{% endblock %}
{# Css Extra #}
{% block appHeader %}
<link rel="stylesheet" href="assets/theme/global/vendor/datatables-bootstrap/dataTables.bootstrap.css">
<link rel="stylesheet" href="assets/theme/global/vendor/datatables-responsive/dataTables.responsive.css">
<link rel="stylesheet" href="assets/theme/assets/examples/css/tables/datatable.css">
{% endblock %}

{# Contenido de la página #}
{% block appBody %}
    {% include 'overall/gestion' with {
		'page_title': 'Duraciones en las Atracciones',
		'page_subtitle': 'Gestion de Duraciones en las Atracciones',
		'panel_title': 'Lista de Duraciones en las Atracciones',
		'data': data,
		'field': ['id_atraccion_duracion', 'atraccion_duracion'],
		'table': ['Id','Duración de la atracción','Acciones'],
		'form_add_name':'atraccionesduraciones_add_form',
		'btn_add_name':'atraccionesduraciones_add_btn',
		'form_edit_name': 'atraccionesduraciones_edit_form',
		'btn_edit_name': 'atraccionesduraciones_edit_btn',
		'element_name': 'atraccion_duracion'
 	} only %}
{% endblock %}

{# scripts extra #}
{% block appFooter %}
	<script src="assets/theme/global/vendor/datatables/jquery.dataTables.js"></script>
	<script src="assets/theme/global/vendor/datatables-bootstrap/dataTables.bootstrap.js"></script>
	<script src="assets/theme/global/vendor/datatables-responsive/dataTables.responsive.js"></script>
	<script src="assets/theme/global/js/Plugin/datatables.js"></script>
	<script src="assets/theme/assets/examples/js/tables/datatable.js"></script>
    <script src="./assets/jscontrollers/atraccionesduraciones/crear.js"></script>
    <script src="./assets/jscontrollers/atraccionesduraciones/editar.js"></script>
{% endblock %}